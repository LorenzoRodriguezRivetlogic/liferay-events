<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.rivetlogic.event.model.Event" />
	<import class="com.rivetlogic.event.model.Location" />
	<import class="com.rivetlogic.event.model.Participant" />
	<import class="com.rivetlogic.event.model.Target" />
	<import class="com.rivetlogic.event.model.Token" />
	<import class="com.rivetlogic.event.model.Type" />
	<class name="com.rivetlogic.event.model.impl.EventImpl" table="rivetlogic_event_Event">
		<cache usage="read-write" />
		<id name="eventId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="calendarBookingId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="calendarId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="location" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="tags" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="eventDate" type="org.hibernate.type.TimestampType" />
		<property name="eventEndDate" type="org.hibernate.type.TimestampType" />
		<property name="privateEvent" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="registrationRequired" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="requiredFullName" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="requiredEmail" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="requiredPhone" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<one-to-one name="imageBlobModel" access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" class="com.rivetlogic.event.model.EventImageBlobModel" cascade="save-update" outer-join="false" constrained="true" />
		<property name="locationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="targetId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="typeId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.rivetlogic.event.model.EventImageBlobModel" table="rivetlogic_event_Event" lazy="true">
		<id name="eventId" column="eventId">
			<generator class="foreign">
				<param name="property">com.rivetlogic.event.model.impl.EventImpl</param>
			</generator>
		</id>
		<property column="image" name="imageBlob" type="blob" />
	</class>
	<class name="com.rivetlogic.event.model.impl.LocationImpl" table="rivetlogic_event_Location">
		<cache usage="read-write" />
		<id name="locationId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.rivetlogic.event.model.impl.ParticipantImpl" table="rivetlogic_event_Participant">
		<cache usage="read-write" />
		<id name="participantId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="eventId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="fullName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phoneNumber" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="companyName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.rivetlogic.event.model.impl.TargetImpl" table="rivetlogic_event_Target">
		<cache usage="read-write" />
		<id name="targetId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.rivetlogic.event.model.impl.TokenImpl" table="rivetlogic_event_Token">
		<cache usage="read-write" />
		<id name="tokenId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="participantId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="expiredDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.rivetlogic.event.model.impl.TypeImpl" table="rivetlogic_event_Type">
		<cache usage="read-write" />
		<id name="typeId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>